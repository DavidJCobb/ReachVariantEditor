<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
   <Type Name="halo::reach::megalo::forge_label">
      <DisplayString Condition="name.target != nullptr">forge_label[{index}] ({name.target-&gt;translations[0].c_str(),s8})</DisplayString>
      <DisplayString Condition="name.target == nullptr &amp;&amp; requirements.flags.value != 0">forge_label[{index}] &lt;unnamed, with requirements&gt;</DisplayString>
      <DisplayString Condition="name.target == nullptr">forge_label[{index}] &lt;no name or name not yet loaded&gt;</DisplayString>
   </Type>
</AutoVisualizer>